{
  "extends": "vercel.json",
  "// --- For backend only --- ": "",
  "functions": {
    "api/index.ts": {
      "runtime": "node18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.ts",
      "headers": {
        "Access-Control-Allow-Origin": "https://aarez-mgnmt.vercel.app",
        "Access-Control-Allow-Methods": "GET,POST,PUT,DELETE,OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type,Authorization,Cookie",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/api/options/(.*)",
      "methods": ["OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "https://aarez-mgnmt.vercel.app",
        "Access-Control-Allow-Methods": "GET,POST,PUT,DELETE,OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type,Authorization,Cookie",
        "Access-Control-Max-Age": "86400",
        "Access-Control-Allow-Credentials": "true"
      },
      "dest": "api/index.ts"
    }
  ]
}
