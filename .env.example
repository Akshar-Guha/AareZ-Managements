# Copy to .env (local dev) and set these on Vercel Project Settings
DATABASE_URL="postgresql://<user>:<password>@<host>/<database>?sslmode=require"
JWT_SECRET="replace-with-long-random-secret"
# Set true for first deploy to auto-create tables; switch to false after tables are created
MIGRATE_ON_START=true
# For local dev CORS
CORS_ORIGIN=http://localhost:5173
