{
  "name": "healthcare-manager-clone",
  "engines": {
    "node": "18.x"
  },
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently -k -n api,web -c blue,green \"npm:dev:server\" \"vite\"",
    "dev:server": "tsx api-local/server.ts",
    "build": "vite build",
    "preview": "vite preview --port 5173 --strictPort",
    "lint": "eslint . || true",
    "start": "vite",
    "knex": "node --loader ts-node/esm ./node_modules/knex/bin/cli.js --knexfile knexfile.ts",
    "migrate:make": "npm run knex migrate:make -x ts",
    "migrate:latest": "npm run knex migrate:latest",
    "migrate:rollback": "npm run knex migrate:rollback",
    "monitor:up": "docker-compose up -d prometheus grafana",
    "monitor:down": "docker-compose down",
    "monitor:logs": "docker-compose logs -f prometheus grafana",
    "axiom:login": "npx @axiomhq/axiom login",
    "axiom:push": "npx @axiomhq/axiom push"
  },
  "dependencies": {
    "vite": "^5.4.2",
    "@vitejs/plugin-react": "^4.3.1",
    "tailwindcss": "^3.4.10",
    "autoprefixer": "^10.4.20",
    "@axiomhq/js": "^1.3.1",
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@neondatabase/serverless": "^0.9.5",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.4.3",
    "clsx": "^2.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^17.2.1",
    "express": "^4.19.2",
    "express-prom-bundle": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "lucide-react": "^0.471.0",
    "pg": "^8.16.3",
    "prom-client": "^15.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "serverless-http": "^3.2.0",
    "sonner": "^1.5.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tesseract.js": "^5.1.0",
    "ts-node": "^10.9.2",
    "uuid": "^9.0.1",
    "zod": "^3.23.8",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/knex": "^0.15.2",
    "@types/node": "^22.5.1",
    "@types/pg": "^8.15.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "concurrently": "^9.0.1",
    "eslint": "^9.9.1",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "postcss": "^8.4.41"
  }
}
