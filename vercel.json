{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node@3.0.0",
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index"
    },
    {
      "src": "^/assets/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/assets/$1"
    },
    {
      "src": "^/.*\\.(js|css|svg|png|jpg|jpeg|gif|webp|ico|woff|woff2)$",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/$0"
    },
    {
      "src": "^/(.*)",
      "dest": "/index.html"
    }
  ]
}
